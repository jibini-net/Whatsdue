@page "/UsersList"
@using Generated
@model Whatsdue.Server.Pages.UsersListModel
@inject Account.IService accounts

<title>MVC</title>

@{
    var allAccounts = await accounts.List(0, 20);
}

<h4 class="mb-4">Loaded from SQL serverside</h4>

@foreach (var account in allAccounts)
{
    <p>@account.FirstName @account.LastName (ID @account.Id): @account.Email</p>
}
<br />

<a href="~/">Back to Blazor</a>
